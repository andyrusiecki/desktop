{
    "layer": "top", // Waybar at top layer
    // "position": "bottom", // Waybar position (top|bottom|left|right)
    "height": 40, // Waybar height (to be removed for auto height)
    // "width": 1280, // Waybar width
    "spacing": 4, // Gaps between modules (4px)
    //"margin-top": 10,
    //"margin-left": 10,
    //"margin-right": 10,

    // Choose the order of the modules
    "modules-left": [
        "hyprland/workspaces",
        "hyprland/window"
        //"hyprland/submap"
    ],
    "modules-center": [
        "custom/playerctl"
    ],
    "modules-right": [
        "custom/updates",
        "idle_inhibitor",
        "pulseaudio",
        "cpu",
        "memory",
        //"network",
        //"bluetooth",
        "temperature",
        "backlight",
        "battery",
        "battery#bat2",
        "custom/dunst",
        "tray",
        "clock",
        "custom/weather"
    ],
    // Modules configuration
    "hyprland/workspaces": {
        "on-click": "activate",
        "all-outputs": true,
        "on-scroll-up": "hyprctl dispatch workspace e-1",
        "on-scroll-down": "hyprctl dispatch workspace e+1"
        // "format": "{icon}",
        // "format-icons": {
        //     "1": "ÔÑ†",
        //     "2": "Ôâ©",
        //     "3": "ÔÑ°",
        //     "4": "ÔÖõ",
        //     "5": "ÔÜº",
        //     "6": "ÔÅµ",
        //     "7": "ÔÑõ",
        //     "urgent": "ÔÅ™",
        //     "focused": "ÔÜí",
        //     "default": "ÔÑë"
        // },
        // "persistent_workspaces": {
        //     "1": [],
        //     "2": [],
        //     "3": [],
        //     "4": [],
        //     "5": [],
        //     "6": [],
        //     "7": []
        // }
    },
    "keyboard-state": {
        "numlock": true,
        "capslock": true,
        "format": "{name} {icon}",
        "format-icons": {
            "locked": "ÔÄ£",
            "unlocked": "ÔÇú"
        }
    },
    "mpd": {
        "format": "{stateIcon} {consumeIcon}{randomIcon}{repeatIcon}{singleIcon}{artist} - {album} - {title} ({elapsedTime:%M:%S}/{totalTime:%M:%S}) ‚∏®{songPosition}|{queueLength}‚∏© {volume}% ÔÄÅ",
        "format-disconnected": "Disconnected ÔÄÅ",
        "format-stopped": "{consumeIcon}{randomIcon}{repeatIcon}{singleIcon}Stopped ÔÄÅ",
        "unknown-tag": "N/A",
        "interval": 2,
        "consume-icons": {
            "on": "ÔÉÑ "
        },
        "random-icons": {
            "off": "<span color=\"#f53c3c\">ÔÅ¥</span> ",
            "on": "ÔÅ¥ "
        },
        "repeat-icons": {
            "on": "ÔÄû "
        },
        "single-icons": {
            "on": "ÔÄû1 "
        },
        "state-icons": {
            "paused": "ÔÅå",
            "playing": "ÔÅã"
        },
        "tooltip-format": "MPD (connected)",
        "tooltip-format-disconnected": "MPD (disconnected)"
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        }
    },
    "tray": {
        "icon-size": 21,
        "spacing": 10
    },
    "clock": {
        // "timezone": "America/New_York",
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format-alt": "{:%Y-%m-%d}"
    },
    "cpu": {
        "format": "Ôãõ {usage}%",
        "tooltip": false,
        "on-click": "kitty -T 'term_float' toolbox run -c sway-extras btop"
    },
    "memory": {
        "format": "ÔÉâ {}%",
        "on-click": "kitty -T 'term_float' toolbox run -c sway-extras btop"
    },
    "temperature": {
        // "thermal-zone": 2,
        // "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
        "critical-threshold": 80,
        // "format-critical": "{temperatureC}¬∞C {icon}",
        "format": "{icon} {temperatureC}¬∞C",
        "format-icons": ["Ôù´", "Ôãâ", "Ôù©"]
    },
    "backlight": {
        // "device": "acpi_video1",
        "format": "{icon} {percent}%",
        "format-icons": ["Óéç", "Óèì", "Óèë", "Óèè", "Óèé", "Óèç", "Óèä", "Óèà", "Óéõ"]
    },
    "battery": {
        "states": {
            // "good": 95,
            "warning": 30,
            "critical": 15
        },
        "format": "{icon} {capacity}%",
        "format-charging": "Ôóß {capacity}% ",
        "format-plugged": "Ôá¶ {capacity}% ",
        "format-alt": "{icon} {time}",
        // "format-good": "", // An empty format will hide the module
        // "format-full": "",
        "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"]
    },
    "battery#bat2": {
        "bat": "BAT2"
    },
    "network": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "interval": 5,
        "format-wifi": "Ôá´ {essid} ({signalStrength}%)",
        "format-ethernet": "Ôûñ {ipaddr}/{cidr}",
        "format-linked": "Ôûñ {ifname} (No IP)",
        "format-disconnected": "‚ö† Disconnected",
        "format-alt": "{ifname}: {ipaddr}/{cidr}",
        "tooltip-format": "Ôûñ {ifname} via {gwaddr}",
        "on-click": "kitty -T 'term_float' fish -c 'nmtui'"
    },
    "bluetooth": {
        "format": "Ôäî {status}",
        "format-connected": "Ôäî {device_alias}",
        "format-connected-battery": "Ôäî {device_alias} {device_battery_percentage}%",
        // "format-device-preference": [ "device1", "device2" ], // preference list deciding the displayed device
        "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
        "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
        "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t{device_battery_percentage}%",
        "on-click": "blueberry"
    },
    "pulseaudio": {
        // "scroll-step": 1, // %, can be a float
        "format": "{icon} {volume}% {format_source}",
        "format-muted": "Û∞ùü mute {format_source}",
        "format-source": "ÔÑ∞ {volume}%",
        "format-source-muted": "ÔÑ± mute",
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "Ôñê",
            "headset": "Ôñê",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": "ÔÄ®"
        },
        "on-click": "pavucontrol"
    },
    "custom/dunst": {
        "format": "{icon} {}",
        "return-type": "json",
        "format-icons": {
            "paused": "Ôá∂",
            "default": "ÔÉ≥"
        },
        "exec": "~/.config/waybar/scripts/dunst.sh",
        "on-click": "dunstctl set-paused toggle; pkill -SIGRTMIN+7 waybar",
        "signal": 7,
        "interval": 5,
        "tooltip": false

    },
    "custom/media": {
        "format": "{icon} {}",
        "return-type": "json",
        "max-length": 40,
        "format-icons": {
            "spotify": "ÔÜº",
            "default": "üéú"
        },
        "escape": true,
        "exec": "~/.config/waybar/scripts/mediaplayer.py 2> /dev/null"
    },
    "custom/playerctl": {
        "format": "{icon} {}",
        "return-type": "json",
        "max-length": 40,
        "format-icons": {
            "spotify": "ÔÜº",
            "default": "üéú"
        },
        "exec": "~/.config/waybar/scripts/playerctl.sh",
        "exec-if": "pgrep spotify",
        "on-click": "playerctl play-pause",
        "restart-interval": 1
    },
    "custom/updates": {
        "format": "{icon} {}",
        "return-type": "json",
        "format-icons": {
            "updates": "Ó¨©",
            "default": ""
        },
        "interval": 3600,
        "exec": "~/.config/waybar/scripts/num_updates.sh",
        "on-click": "kitty -T 'term_float' /bin/bash -c '~/.config/waybar/scripts/run_update.sh'; pkill -SIGRTMIN+8 waybar",
        "signal": 8
    },
    "custom/weather": {
        "format": "{} ¬∞",
        "tooltip": true,
        "interval": 3600,
        "exec": "wttrbar --ampm --fahrenheit --date-format '%m-%d-%Y'",
        "return-type": "json"
    }
}
